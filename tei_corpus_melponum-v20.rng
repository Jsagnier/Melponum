						<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
							>Latin</a:documentation>
						<value>frm</value>
						<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Middle
							French (ca 1400-1600)</a:documentation>
						<value>fr</value>
						<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
							>French</a:documentation>
						<value>x-ghl</value>
						<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">langue
							hybride construite à partir de grec, hébreu et latin (présente dans Mystère de
							Loupvent, 1548)</a:documentation>
						<value>ar</value>
						<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
							>Arabic</a:documentation>
						<value>es</value>
						<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
							>Spanish</a:documentation>
						<data type="token">
							<param name="pattern">[^\p{C}\p{Z}]+</param>
						</data>
					</choice>
				</choice>
			</attribute>
		</optional>
	</define>
