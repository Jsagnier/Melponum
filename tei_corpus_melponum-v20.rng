						<ref name="tei_model.global"/>
					</choice>
				</zeroOrMore>
				<optional xmlns:rng="http://relaxng.org/ns/structure/1.0">
					<group>
						<choice>
							<group>
								<ref name="tei_model.div1Like"/>
								<zeroOrMore>
									<choice>
										<ref name="tei_model.div1Like"/>
										<ref name="tei_model.frontPart"/>
										<ref name="tei_model.global"/>
									</choice>
								</zeroOrMore>
							</group>
							<group>
								<ref name="tei_model.divLike"/>
								<zeroOrMore>
									<choice>
										<ref name="tei_model.divLike"/>
										<ref name="tei_model.frontPart"/>
										<ref name="tei_model.global"/>
									</choice>
