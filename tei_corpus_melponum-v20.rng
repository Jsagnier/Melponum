	<define name="tei_att.global.attribute.xmllang">
		<!-- Modification : 
		- ajout d'une liste de valeurs acceptées-->
		<!-- MM : docummentation : https://www.iana.org/assignments/language-subtag-registry/language-subtag-registry -->
		<optional>
			<attribute name="xml:lang">
				<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(language)
